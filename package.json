{
  "name": "vadistic.github.io",
  "description": "Personal website",
  "version": "1.0.0",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "keywords": [
    "gatsby",
    "typescript",
    "react",
    "developement",
    "frontend"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tylergreulich/gatsby-typescript-mdx-prismjs-starter"
  },
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "format": "prettier --write './src/*/**.{js,ts,tsx}'",
    "lint": "tslint src/*/**.{ts,tsx}",
    "lint:fix": "tslint --fix src/*/**.{ts,tsx}",
    "codegen:schema": "yarn apollo schema:download --endpoint='http://localhost:8000/___graphql' types/schema.json",
    "codegen:generate": "yarn apollo codegen:generate --target='typescript' --tagName='graphql' --queries='./src/**/*.tsx' --schema='./types/schema.json' generated",
    "codegen:clean": "rm -rf src/**/generated",
    "codegen:indicies": "sh scripts/codegen-indicies.sh",
    "codegen": "yarn codegen:clean && yarn codegen:schema && yarn codegen:generate && yarn codegen:indicies",
    "test": "jest"
  },
  "dependencies": {
    "@svgr/webpack": "^4.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.0.87",
    "gatsby-image": "^2.0.25",
    "gatsby-mdx": "0.2.0",
    "gatsby-plugin-manifest": "^2.0.13",
    "gatsby-plugin-offline": "^2.0.21",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-sharp": "^2.0.17",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-plugin-svgr": "^2.0.1",
    "gatsby-plugin-typescript": "^2.0.3",
    "gatsby-remark-prismjs": "^3.2.0",
    "gatsby-source-filesystem": "^2.0.13",
    "gatsby-transformer-remark": "^2.1.20",
    "gatsby-transformer-sharp": "^2.1.10",
    "grommet": "^2.3.1",
    "polished": "^2.3.3",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-testing-library": "^5.4.4",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@mdx-js/loader": "^0.16.6",
    "@mdx-js/mdx": "^0.16.6",
    "@mdx-js/tag": "^0.16.6",
    "@types/jest": "^23.3.12",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.5",
    "apollo": "1.9.2",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "loader-utils": "^1.2.3",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  },
  "resolutions": {
    "graphql": "0.13.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__tests__/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
