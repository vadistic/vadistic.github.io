{
  "name": "vadistic-website",
  "description": "Personal website",
  "version": "0.1.0",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "keywords": [
    "gatsby",
    "typescript",
    "react",
    "developement",
    "frontend"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vadistic/website"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format:all": "prettier --write -l --no-color './{src,types}/**/*.{js,ts,tsx,mdx}'",
    "format": "pretty-quick --staged",
    "lint": "tslint -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "pre-commit": "yarn format && yarn lint && yarn typecheck",
    "apollo:check": "apollo service:check --endpoint http://localhost:8000/___graphql",
    "apollo:push": "apollo service:push --endpoint http://localhost:8000/___graphql",
    "apollo:schema": "apollo service:download --endpoint 'http://localhost:8000/___graphql' src/generated/schema.json",
    "apollo:codegen": "apollo client:codegen ./src/generated/graphql.ts --outputFlat --target typescript",
    "test": "echo 'No tests specified'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint && yarn typecheck"
    }
  },
  "dependencies": {
    "@mdx-js/loader": "^0.16.8",
    "@mdx-js/mdx": "^0.16.8",
    "@mdx-js/tag": "^0.16.8",
    "@svgr/webpack": "^4.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.0.111",
    "gatsby-image": "^2.0.29",
    "gatsby-mdx": "0.3.5",
    "gatsby-plugin-layout": "^1.0.12",
    "gatsby-plugin-manifest": "^2.0.17",
    "gatsby-plugin-offline": "^2.0.22",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-sharp": "^2.0.20",
    "gatsby-plugin-styled-components": "^3.0.5",
    "gatsby-plugin-svgr": "^2.0.1",
    "gatsby-plugin-typescript": "^2.0.6",
    "gatsby-source-filesystem": "^2.0.20",
    "gatsby-transformer-remark": "^2.2.3",
    "gatsby-transformer-sharp": "^2.1.13",
    "graphql": "^14.1.1",
    "grommet": "^2.3.1",
    "polished": "^2.3.3",
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@types/grommet-icons": "vadistic/types#types-grommet-icons-v4.1.0-2-gitpkg",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.6",
    "apollo": "^2.4.2",
    "dotenv": "^6.2.0",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "jest-dom": "^3.0.1",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.8.1",
    "react-testing-library": "^5.4.4",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.1"
  },
  "resolutions": {
    "graphql": "14.1.1"
  }
}
