{
  "name": "vadistic-website",
  "description": "Personal website",
  "version": "0.1.0",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "keywords": [
    "gatsby",
    "typescript",
    "react",
    "developement",
    "frontend"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vadistic/website"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "dev-m": "gatsby develop --host $(ifconfig | awk '/inet 192.168.[0-9]+.[0-9]+/{print $2}') --port 8080",
    "serve": "serve public",
    "format:all": "prettier --write -l --no-color './{src,types}/**/*.{js,ts,tsx,md,mdx}'",
    "format": "pretty-quick --staged",
    "lint": "tslint -p .",
    "typecheck": "tsc -p . --noEmit",
    "pre-commit": "yarn format && yarn lint && yarn typecheck",
    "apollo:check": "apollo service:check --endpoint http://localhost:8000/___graphql",
    "apollo:push": "apollo service:push --endpoint http://localhost:8000/___graphql",
    "apollo:schema": "apollo service:download --endpoint 'http://localhost:8000/___graphql' src/generated/schema.json",
    "apollo:codegen": "apollo client:codegen ./src/generated/graphql.ts --outputFlat --target typescript",
    "test": "echo 'No tests specified'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint && yarn typecheck"
    }
  },
  "dependencies": {
    "@mdx-js/mdx": "^0.20.1",
    "@mdx-js/tag": "^0.18.0",
    "@svgr/webpack": "^4.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.1.19",
    "gatsby-image": "^2.0.30",
    "gatsby-mdx": "0.4.0",
    "gatsby-plugin-layout": "^1.0.12",
    "gatsby-plugin-manifest": "^2.0.20",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.7",
    "gatsby-plugin-sharp": "^2.0.23",
    "gatsby-plugin-styled-components": "^3.0.6",
    "gatsby-plugin-svgr": "^2.0.1",
    "gatsby-plugin-typescript": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.23",
    "gatsby-transformer-remark": "^2.3.0",
    "gatsby-transformer-sharp": "^2.1.15",
    "graphql": "^14.1.1",
    "grommet": "^2.5.5",
    "mdx.macro": "^0.2.8",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3",
    "typeface-rubik": "^0.0.72"
  },
  "devDependencies": {
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.11",
    "apollo": "^2.5.3",
    "dotenv": "^6.2.0",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.8.1",
    "react-testing-library": "^6.0.0",
    "serve": "^10.1.2",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333"
  },
  "resolutions": {
    "graphql": "14.1.1"
  }
}
