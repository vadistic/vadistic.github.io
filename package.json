{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "babel-plugin-emotion": "^9.1.2",
    "babel-plugin-tailwind": "^0.1.10",
    "classnames": "^2.2.5",
    "emotion": "^9.1.3",
    "emotion-server": "^9.1.3",
    "emotion-theming": "^9.1.2",
    "gatsby": "^2.0.0-alpha.48",
    "gatsby-link": "^1.6.44",
    "gatsby-plugin-emotion": "^2.0.0-alpha.8",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-typescript": "vadistic/gatsby-plugin-typescript",
    "gatsby-remark-prismjs": "^2.0.3",
    "gatsby-source-filesystem": "^2.0.0-alpha.15",
    "gatsby-transformer-remark": "^2.0.1-13",
    "prismjs": "^1.14.0",
    "ramda": "^0.25.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-emotion": "^9.1.3",
    "react-helmet": "^5.2.0",
    "recompose": "^0.27.1",
    "styled-system": "^2.2.5",
    "tailwindcss": "^0.5.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:css && gatsby build",
    "generate:tw-typings": "node utils/tw-typings.js",
    "css:tailwindcss": "yarn postcss tailwind.css -o tailwind.custom.css",
    "css:preflight": "yarn postcss ./src/styles/preflight.css -o ./src/styles/preflight.build.css",
    "css:components": "yarn postcss ./src/styles/components.css -o ./src/styles/components.build.css",
    "css:prism": "yarn postcss ./src/styles/prism.css -o ./src/styles/prism.build.css",
    "build:css": "yarn css:tailwindcss && yarn css:preflight && yarn css:components && yarn css:prism",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/ramda": "^0.25.32",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "markdownlint": "^0.10.0",
    "postcss-cli": "^5.0.0",
    "prettier": "^1.12.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.0-rc"
  }
}
