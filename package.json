{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "babel-plugin-emotion": "^9.1.2",
    "babel-plugin-tailwind": "^0.1.10",
    "classnames": "^2.2.5",
    "emotion": "^9.1.3",
    "emotion-server": "^9.1.3",
    "emotion-theming": "^9.1.2",
    "gatsby": "^2.0.0-alpha.42",
    "gatsby-link": "^1.6.44",
    "gatsby-plugin-emotion": "^2.0.0-alpha.8",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-typescript": "vadistic/gatsby-plugin-typescript",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-emotion": "^9.1.3",
    "react-helmet": "^5.2.0",
    "tailwindcss": "^0.5.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:css && gatsby build",
    "generate:tailwindcss": "yarn postcss tailwind.css -o tailwind.custom.css",
    "generate:preflight": "yarn postcss ./src/styles/preflight.css -o ./src/styles/preflight.build.css",
    "generate:css": "yarn generate:tailwindcss && yarn generate:preflight",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "postcss-cli": "^5.0.0",
    "prettier": "^1.12.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.0-rc"
  }
}
