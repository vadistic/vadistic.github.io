{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "babel-plugin-tailwind": "^0.1.10",
    "emotion": "^9.1.3",
    "emotion-server": "^9.1.3",
    "emotion-theming": "^9.1.2",
    "gatsby": "^2.0.0-alpha.42",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-emotion": "^1.1.16",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-typescript": "vadistic/gatsby-plugin-typescript",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-emotion": "^9.1.3",
    "react-helmet": "^5.2.0",
    "tailwindcss": "^0.5.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:css && gatsby build",
    "build:css-preflight": "tailwind build -c ./src/styles/tailwind.js -o ./src/styles/preflight.build.css ./src/styles/preflight.css",
    "build:css-utilities": "tailwind build -c ./src/styles/tailwind.js -o ./src/styles/utilities.build.css ./src/styles/utilities.css",
    "build:css": "yarn build:css-preflight && yarn build:css-utilities",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "prettier": "^1.12.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.0-rc"
  }
}
